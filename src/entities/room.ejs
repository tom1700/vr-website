<a-entity
    id="room"
    position="0 0 0"
>
    <a-plane
        id="floor"
        height="<%- roomSize.z -%>"
        width="<%- roomSize.x -%>"
        rotation="-90 0 0"
        position="0 0 0"
        repeat="<%- roomSize.z / 2 -%> <%- roomSize.z / 2 -%>"
        src="#<%- ids.floorTexture -%>"
    ></a-plane>
    <a-plane
        id="ceiling"
        height="<%- roomSize.z -%>"
        width="<%- roomSize.x -%>"
        rotation="90 0 0"
        color="#FFF"
        position="0 <%- roomSize.y -%> 0"
    ></a-plane>
    <a-box
        id="back-wall-1"
        height="<%- roomSize.y -%>"
        width="4.61"
        depth="0.16"
        color="#AAA"
        position="-2.29 1.75 -4.92"
        dynamic-color
        data-raycastable
    ></a-box>
    <a-box
        id="back-wall-2"
        height="<%- roomSize.y -%>"
        width="4.58"
        depth="0.16"
        color="#AAA"
        position="2.29 1.75 -4.92"
        dynamic-color
        data-raycastable
    ></a-box>
    <a-box
        id="back-wall-3"
        height="1.37"
        width="4.58"
        depth="0.16"
        color="#AAA"
        position="0 2.82567 -4.92"
        dynamic-color
        data-raycastable
    ></a-box>
    <a-plane
        height="<%- roomSize.y -%>"
        width="<%- roomSize.x -%>"
        rotation="0 180 0"
        color="#AAA"
        position="0 <%- roomSize.y / 2 -%>  <%- roomSize.z / 2 -%> "
        dynamic-color
        data-raycastable
    ></a-plane>
    <a-plane
        height="<%- roomSize.y -%>"
        width="<%- roomSize.z -%>"
        rotation="0 90 0"
        color="#AAA"
        position="<%- -roomSize.x / 2 -%>  <%- roomSize.y / 2 -%>  0"
        dynamic-color
        data-raycastable
    ></a-plane>
    <a-plane
        height="<%- roomSize.y -%>"
        width="<%- roomSize.z -%>"
        rotation="0 -90 0"
        color="#AAA"
        position="<%- roomSize.x / 2 -%>  <%- roomSize.y / 2 -%>  0"
        dynamic-color
        data-raycastable
    ></a-plane>
    <a-entity 
        obj-model="obj: #<%- ids.doorsObject -%>; mtl: #<%- ids.doorsMaterial -%>"
        position="0 0  <%- -4.953 -%> "
        rotation="0 180 0"
        width="1"
        height="2"
        depth="0.16"
    ></a-entity>
    <!-- scale="0.001 0.001 0.001" -->
    <a-entity 
        obj-model="obj: #<%- ids.windowObject -%>; mtl: #<%- ids.windowMaterial -%>"
        position="-2 1.8  <%- roomSize.z / 2 -%> "
        rotation="0 180 0"
    ></a-entity>
    <a-entity 
        obj-model="obj: #<%- ids.windowObject -%>; mtl: #<%- ids.windowMaterial -%>"
        position="2 1.8  <%- roomSize.z / 2 -%> "
        rotation="0 180 0"
    ></a-entity>
    <a-entity 
        obj-model="obj: #<%- ids.lampObject -%>; mtl: #<%- ids.lampMaterial -%>"
        position="-2 <%- roomSize.y -%>  0"
        rotation="0 180 0"
    ></a-entity>
    <a-entity 
        obj-model="obj: #<%- ids.lampObject -%>; mtl: #<%- ids.lampMaterial -%>"
        position="2 1  0"
        rotation="0 180 0"
    ></a-entity>
</a-entity>
